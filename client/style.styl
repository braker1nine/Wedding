
vendor(prop, args)
	-webkit-{prop} args
	-moz-{prop} args
	{prop} args

border-radius()
	vendor('border-radius', arguments)

box-sizing()
	vendor('box-sizing', arguments)

*
	box-sizing border-box
	padding 0
	border none
	margin 0

header
	text-transform uppercase
	color #402F14
	h1
		display: inline-block
		text-shadow: 0px -1px 1px rgba(121, 119, 119, 0.8)
	nav
		ul
			display: inline-block
			padding-left: 40px
			float: right
			border-bottom: 1px solid rgb(255,244,226)
			li
				color:white
				display:inline-block
				list-style: none
				padding: 10px
				a
					text-decoration:none
					text-shadow: 0px -1px 1px rgb(12,12,12)
